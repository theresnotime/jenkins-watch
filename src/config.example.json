{
    "debug": false,
    "interval": 60,
    "api": {
        "url": "https://jenkins.api.url"
    },
    "alerts":{
        "pushover": {
            "enabled": true,
            "url": "https://api.pushover.net/1/messages.json",
            "token": "application_token",
            "user": "user_token"
        }
    },
    "jobs": {
        "job-name": {
            "name": "job-name",
            "url": "https://link.to.jenkins.job/",
            "view": "jenkins-view",
            "overdue": 1500,
            "confirms": 2,
            "alerts": {
                "pushover": {
                    "device": "device_name",
                    "priority": 0
                }
            }
        },
        "job-with-no-alert": {
            "name": "job-with-no-alert",
            "url": "https://link.to.jenkins.job/",
            "view": "jenkins-view",
            "overdue": 1500,
            "confirms": 2,
            "alerts": []
        }
    }
}